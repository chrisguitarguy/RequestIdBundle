{
    "name": "chrisguitarguy/request-id-bundle",
    "description": "Add request IDs to to your Symfony requests.",
    "keywords": ["symfony", "request id"],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "symfony/framework-bundle": "^6.4.22 || ^7.0",
        "ramsey/uuid": "^4.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "symfony/monolog-bundle": "^3.10",
        "twig/twig": "^3.9",
        "symfony/twig-bundle": "^6.4.13 || ^7.0",
        "symfony/twig-bridge": "^6.4.22 || ^7.0",
        "symfony/browser-kit": "^6.4.19 || ^7.0",
        "symfony/dom-crawler": "^6.4.19 || ^7.0",
        "symfony/css-selector": "^6.4.13 || ^7.0",
        "symfony/phpunit-bridge": "^6.4.16 || ^7.0",
        "symfony/yaml": "^6.4.21 || ^7.0",
        "symfony/monolog-bridge": "^6.4.13 || ^7.0",
        "symfony/templating": "^6.4.13",
        "symfony/event-dispatcher": "^6.4.13 || ^7.0",
        "symfony/event-dispatcher-contracts": "^3.5",
        "symfony/http-foundation": "^6.4.22 || ^7.0",
        "symfony/http-kernel": "^6.4.22 || ^7.0",
        "symfony/routing": "^6.4.22 || ^7.0",
        "symfony/config": "^6.4.22 || ^7.0",
        "symfony/var-exporter": "^6.4.22 || ^7.0",
        "symfony/error-handler": "^6.4.22 || ^7.0",
        "symfony/translation": "^6.4.22 || ^7.0",
        "symfony/translation-contracts": "^3.5",
        "symfony/filesystem": "^6.4.13 || ^7.0",
        "symfony/cache": "^6.4.21 || ^7.0",
        "symfony/cache-contracts": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Chrisguitarguy\\RequestId\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrisguitarguy\\RequestId\\": [
                "test/unit/",
                "test/acceptance/"
            ]
        },
        "files": [
            "test/acceptance/app/TestKernel.php"
        ]
    },
    "conflict": {
        "twig/twig": "<2.7"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "composer/package-versions-deprecated": true
        }
    }
}
